	<script>
		(function () {
		  const form = document.getElementById('requestForm');
		  const alertContainer = document.getElementById('formAlertContainer');
		  const FORMSPREE_URL = 'https://formspree.io/f/mjkejvrr';

		  function showAlert(type, message, autoClose = true, timeout = 6000) {
			// Clear any previous alerts
			alertContainer.innerHTML = '';

			const wrapper = document.createElement('div');
			wrapper.innerHTML = `
				  <div class="alert alert-${type} alert-dismissible fade show" style="float: right;width: 100%;text-align: center;" role="alert">
				${message}
				<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
			  </div>
			`;
			alertContainer.appendChild(wrapper);

			// Move focus to the alert for accessibility
			const alertEl = alertContainer.querySelector('.alert');
			if (alertEl) alertEl.focus?.();

			if (autoClose) {
			  setTimeout(() => {
				// Bootstrap's dismiss via button (fade out)
				if (alertEl) {
				  const bsAlert = bootstrap.Alert.getOrCreateInstance(alertEl);
				  bsAlert.close();
				}
			  }, timeout);
			}
		  }

		  form.addEventListener('submit', function (e) {
			e.preventDefault();

			// client-side validation (uses built-in validity)
			if (!form.checkValidity()) {
			  // show browser built-in messages
			  form.reportValidity();
			  return;
			}

			// Prepare form data
			const formData = new FormData(form);

			// Optionally add a subject or extra fields:
			// formData.append('_subject', 'New callback request');

			// Disable submit to prevent multiple clicks
			const submitBtn = form.querySelector('[type="submit"]');
			submitBtn.disabled = true;
			submitBtn.classList.add('disabled');

			fetch(FORMSPREE_URL, {
			  method: 'POST',
			  headers: {
				'Accept': 'application/json'
			  },
			  body: formData
			})
			.then(response => {
			  submitBtn.disabled = false;
			  submitBtn.classList.remove('disabled');

			  if (response.ok) {
				return response.json().catch(() => ({})); // some responses may be empty
			  } else {
				// Non-2xx returned
				return response.json().then(err => Promise.reject(err)).catch(() => Promise.reject({ message: 'Submission failed.' }));
			  }
			})
			.then(data => {
			  // Successful submission
			  showAlert('success', 'Thanks — your request has been sent. We will contact you shortly.');

			  // Clear the form fields
			  form.reset();
			})
			.catch(err => {
			  console.error('Form submission error:', err);
			  showAlert('danger', 'Sorry — there was a problem sending your request. Please try again later.');
			});
		  });
		})();
	</script>
}